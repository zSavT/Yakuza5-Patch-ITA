msgctxt "174\t234\t60\t\t \t "
msgid "You get to go first."
msgstr "A te la prima mossa."

msgctxt "332\t392\t60\t　\t　\t "
msgid "Die."
msgstr "Muori."

msgctxt "632\t692\t60\t　\t　\t "
msgid "Ka-chan?"
msgstr "Ka-chan?"

msgctxt "758\t818\t60\t　\t　\t "
msgid "Made it..."
msgstr "Ce l'ho fatta..."

msgctxt "891\t941\t50\t　\t　\t60ｆ以下です"
msgid "Ka-chan!?"
msgstr "Ka-chan!?"

msgctxt "942\t1006\t64\t　\t　\t "
msgid "Hey! Ka-chan!"
msgstr "Ehi! Ka-chan"

msgctxt "1011\t1071\t60\t　\t　\t "
msgid "Don't you give up!"
msgstr "Non mollare"

msgctxt "1142\t1278\t136\t　\t　\t "
msgid ""
"Why, Ka-chan!?\r\n"
"Why'd ya go and do that!?"
msgstr ""
"Perché, Ka-chan!?\n"
"Perché l'hai fatto!?"

msgctxt "1301\t1415\t114\t　\t　\t "
msgid "Without you, Aniki..."
msgstr "Senza di te, Aniki..."

msgctxt "1513\t1573\t60\t　\t　\t "
msgid "The Omi would..."
msgstr "L'Omi avrebbe..."

msgctxt "1650\t1825\t175\t　\t　\t "
msgid "The eighth chairman... should be you."
msgstr "L'ottavo presidente... dovresti essere tu."

msgctxt "1900\t1960\t60\t　\t　\t "
msgid "It has to be."
msgstr "Deve essere così."

msgctxt "2003\t2127\t124\t　\t　\t "
msgid ""
"I hear ya! Just don't die on me!\r\n"
"Ka-chan, don't!"
msgstr ""
"Ho capito! Non mi morire!\n"
"Ka-chan, non farlo"

msgctxt "2208\t2315\t107\t　\t　\t "
msgid "Ka-chan! C'mon, Ka-chan!"
msgstr "Ka-chan! Dai, Ka-chan"

msgctxt "3085\t3145\t60\t　\t　\t "
msgid "I'll never forget this."
msgstr "Non lo dimenticherò mai."

msgctxt "3180\t3240\t60\t　\t　\t "
msgid "You're going to pay."
msgstr "La pagherai."

msgctxt "3259\t3319\t60\t分\t　\t "
msgid "That's a promise!"
msgstr "È una promessa"

msgctxt "3401\t3487\t86\t　\t　\t "
msgid "I'm headin' over there to settle this right now!"
msgstr "Adesso vado di là a sistemare questa faccenda"

msgctxt "3577\t3656\t79\t　\t　\t "
msgid "Are ya now? Dumbass."
msgstr "Ah, davvero? Deficiente."

msgctxt "3661\t3799\t138\t　\t　\t "
msgid ""
"Pretty big talk for a guy\r\n"
"fallin' apart at the seams."
msgstr ""
"Hai un bel coraggio a parlare\n"
"conciato in quel modo."

msgctxt "3984\t4052\t68\t　\t　\t "
msgid "I've got one shot left."
msgstr "Mi è rimasto un solo colpo."

msgctxt "4088\t4212\t124\t分\t　\t "
msgid "If ya wanna volunteer, raise your hand."
msgstr "Se qualcuno vuole farsi avanti, alzi la mano."

msgctxt "4272\t4332\t60\t　\t　\t "
msgid "How about me?"
msgstr "Che ne dici di me?"

msgctxt "4421\t4481\t60\t　\t　\t "
msgid "Haven't seen you in a month."
msgstr "Non ti vedo da un mese."

msgctxt "4625\t4682\t57\t　\t　\t60ｆ以下です"
msgid "Chairman Kurosawa."
msgstr "Presidente Kurosawa."

msgctxt "4683\t4770\t87\t　\t　\t "
msgid "Daigo Dojima..."
msgstr "Daigo Dojima..."

msgctxt "4805\t4865\t60\t　\t　\t "
msgid "Daigo!?"
msgstr "Daigo!?"
