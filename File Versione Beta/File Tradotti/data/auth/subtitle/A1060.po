msgctxt "1926\t1955\t29\t\t60ｆ以下です\t "
msgid "Here are my fares."
msgstr "Ecco i miei incassi."

msgctxt "1956\t2049\t93\t　\t \t "
msgid ""
"Oh, Suzuki-san!\r\n"
"Good to see you."
msgstr ""
"Oh, Suzuki-san!\n"
"Lieto di vederti."

msgctxt "2151\t2200\t49\t　\t60ｆ以下です\t "
msgid "Been waitin' for ya!"
msgstr "Ti stavo aspettando"

msgctxt "2201\t2292\t91\t　\t \t "
msgid ""
"Nakajima, sir.\r\n"
"Good evening."
msgstr ""
"Signor Nakajima.\n"
"Buonasera."

msgctxt "2315\t2416\t101\t　\t \t "
msgid ""
"Got somethin' to talk to you about.\r\n"
"Mind steppin' into my \"office\"?"
msgstr ""
"Devo parlarti di una cosa.\n"
"Ti va di fare un salto nel mio \"ufficio\"?"
