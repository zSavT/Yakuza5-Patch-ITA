msgctxt "210\t292\t82\t\t \t "
msgid "You're tough. Real tough."
msgstr "Sei tosto. Davvero tosto."

msgctxt "677\t737\t60\t　\t　\t "
msgid "Ogita-san..."
msgstr "Ogita-san..."

msgctxt "1175\t1272\t97\t　\t　\t "
msgid ""
"I'll remember you.\r\n"
"You can count on it."
msgstr ""
"Mi ricorderò di te.\n"
"Puoi contarci."

msgctxt "1553\t1613\t60\t　\t　\t "
msgid "Horie-san!"
msgstr "Horie-san"

msgctxt "1637\t1705\t68\t　\t　\t "
msgid "Horie-san? Horie-san!"
msgstr "Horie-san? Horie-san"

msgctxt "1715\t1775\t60\t　\t　\t "
msgid "Why...?"
msgstr "Perché...?"

msgctxt "1884\t1985\t101\t　\t　\t "
msgid "I think that Ogita guy did this to him."
msgstr "Penso che sia stato quel tizio, Ogita, a fargli questo."

msgctxt "2023\t2083\t60\t　\t　\t "
msgid "Ogita-san!?"
msgstr "Ogita-san!?"

msgctxt "2084\t2166\t82\t　\t　\t "
msgid ""
"First things first, we need to\r"
"\r\n"
"get Horie-chan to a hospital."
msgstr ""
"Prima di tutto, dobbiamo\n"
"portare Horie-chan in ospedale."

msgctxt "2174\t2234\t60\t分\t　\t "
msgid "His life depends on it."
msgstr "Ne va della sua vita."
