msgctxt "292\t352\t60\t\t \t "
msgid "Horie-chan."
msgstr "Horie-chan."

msgctxt "427\t512\t85\t　\t \t "
msgid "Akiyama-san! You startled me."
msgstr "Akiyama-san! Mi hai fatto prendere un colpo."

msgctxt "522\t614\t92\t　\t\t"
msgid ""
"I only just heard from Hana that\r"
"\r\n"
"you were down here in Osaka."
msgstr ""
"Ho appena saputo da Hana che\n"
"eri qui a Osaka."

msgctxt "627\t759\t132\t改行位置修正\t \t "
msgid ""
"I never thought we'd be\r"
"\r\n"
"seeing each other so soon."
msgstr ""
"Non avrei mai pensato di rivederci\n"
"così presto."
