msgctxt "189\t249\t60\t\t\t "
msgid "Get back here!"
msgstr "Torna qui!"

msgctxt "285\t345\t60\t　\t　\t "
msgid "Akiyama-san!"
msgstr "Akiyama-san!"

msgctxt "749\t797\t48\t　\t　\t60ｆ以下です"
msgid "Who the shit're you!?"
msgstr "Chi cazzo sei!?"

msgctxt "798\t858\t60\t　\t　\t "
msgid "I should ask you the same thing."
msgstr "Dovrei farti io la stessa domanda."

msgctxt "944\t1004\t60\t　\t　\t "
msgid "Horie-chan!"
msgstr "Horie-chan"

msgctxt "1076\t1136\t60\t　\t　\t "
msgid "So it was you."
msgstr "Allora eri tu."

msgctxt "1183\t1247\t64\t　\t　\t "
msgid "You must be the one who killed Park-san, too!"
msgstr "Sei stato tu a uccidere anche Park-san"

msgctxt "1275\t1327\t52\t　\t　\t60ｆ以下です"
msgid "No!"
msgstr "No!"

msgctxt "1328\t1367\t39\t　\t　\t60ｆ以下です"
msgid "It wasn't me!"
msgstr "Non sono stato io"

msgctxt "1368\t1434\t66\t分\t　\t "
msgid "They made me do it, I swear!"
msgstr "Mi hanno costretto, lo giuro"

msgctxt "1442\t1502\t60\t　\t　\t "
msgid "Who did?"
msgstr "Chi è stato?"

msgctxt "1527\t1665\t138\t　\t　\t "
msgid ""
"Please, just lemme go!\r\n"
"I didn't do anythin' wrong!"
msgstr ""
"Ti prego, lasciami andare!\n"
"Non ho fatto niente di male"

msgctxt "1667\t1733\t66\t　\t　\t "
msgid "Like I'm gonna buy that!"
msgstr "Come se me la bevessi"

msgctxt "1853\t1963\t110\t　\t　\t "
msgid "Fine then. Only one way left to go."
msgstr "Bene, allora. Resta solo una cosa da fare."

msgctxt "2016\t2076\t60\t　\t　\t "
msgid "Can't say I didn't see that coming."
msgstr "Me l'aspettavo."
