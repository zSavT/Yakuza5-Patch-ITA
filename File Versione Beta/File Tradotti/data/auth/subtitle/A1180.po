msgctxt "768\t816\t48\t\t　\t60ｆ以下です"
msgid "Just a second!"
msgstr "Solo un secondo!"

msgctxt "817\t946\t129\t　\t　\t "
msgid ""
"Hold on, Mayumi.\r\n"
"Sit tight, I got it."
msgstr ""
"Aspetta, Mayumi.\n"
"Stai ferma, ci penso io."

msgctxt "1034\t1094\t60\t　\t　\t "
msgid "What's wrong!?"
msgstr "Che succede!?"

msgctxt "1393\t1473\t80\t　\t　\t "
msgid "You've got to help us, Kir⑬"
msgstr "Devi aiutarci, Kir⑬"

msgctxt "1474\t1534\t60\t分\t　\t "
msgid "I mean, Suzuki-san!"
msgstr "Volevo dire, Suzuki-san"

msgctxt "1545\t1614\t69\t　\t　\t "
msgid "How'd you find me here?"
msgstr "Come avete fatto a trovarmi qui?"

msgctxt "1636\t1700\t64\t　\t　\t "
msgid "I can explain later."
msgstr "Posso spiegare dopo."

msgctxt "1717\t1771\t54\t分\t　\t60ｆ以下です"
msgid "Right now, my aniki..."
msgstr "Adesso, il mio aniki..."

msgctxt "1772\t1861\t89\t分\t　\t "
msgid "Morinaga-aniki needs somewhere to rest!"
msgstr "L'aniki Morinaga ha bisogno di un posto dove riposare"

msgctxt "1921\t1981\t60\t　\t　\t "
msgid "Sorry for this."
msgstr "Scusa per questo."

msgctxt "1982\t2160\t178\t分\t　\t "
msgid ""
"I told him not to impose\r\n"
"on you, Suzuki-san..."
msgstr ""
"Gli ho detto di non\n"
"disturbarti, Suzuki-san..."

msgctxt "2171\t2209\t38\t\t　\t60ｆ以下です"
msgid "Aniki?"
msgstr "Aniki?"

msgctxt "2210\t2288\t78\t分\t　\t "
msgid "Stay with me, Aniki!"
msgstr "Resta con me, Aniki"
