msgctxt "323\t393\t70\t\t \t "
msgid "Will you be home late again tonight?"
msgstr "Farai tardi anche stasera?"

msgctxt "404\t494\t90\t　\t　\t "
msgid "Yeah. I'm on the midnight shift."
msgstr "Già. Faccio il turno di notte."

msgctxt "519\t583\t64\t　\t　\t "
msgid ""
"Maybe you should stay at\r"
"\r\n"
"your own place for a change."
msgstr ""
"Forse dovresti stare a\n"
"casa tua, tanto per cambiare."

msgctxt "599\t658\t59\t分\t　\t60ｆ以下です"
msgid "I want to be alone tonight."
msgstr "Voglio stare da solo stasera."

msgctxt "659\t732\t73\t　\t　\t "
msgid "Oh, come on."
msgstr "Maddai!"

msgctxt "771\t880\t109\t　\t　\t "
msgid ""
"I can tell you're happier\r"
"\r\n"
"when I'm around, Suzuki-san."
msgstr ""
"Lo so che sei più felice\n"
"quando ci sono io, Suzuki-san."

msgctxt "889\t949\t60\t　\t　\t "
msgid "I don't know what you mean."
msgstr "Non so di cosa parli."

msgctxt "1065\t1125\t60\t　\t　\t "
msgid "Your tie's crooked."
msgstr "Hai la cravatta storta."

msgctxt "1226\t1291\t65\t　\t　\t "
msgid "There. All better."
msgstr "Ecco. Molto meglio."

msgctxt "1835\t1889\t54\t　\t　\t60ｆ以下です"
msgid "What's wrong?"
msgstr "Che succede?"

msgctxt "1890\t1977\t87\t　\t　\t "
msgid ""
"It's nothing.\r"
"\r\n"
"I should go."
msgstr ""
"Non è niente.\n"
"Devo andare."

msgctxt "2053\t2113\t60\t　\t　\t "
msgid "Have a good day!"
msgstr "Buona giornata"
