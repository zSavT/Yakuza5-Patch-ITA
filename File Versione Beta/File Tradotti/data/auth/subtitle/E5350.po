msgctxt "658\t718\t60\t\t \t "
msgid "Kurosawa."
msgstr "Kurosawa."

msgctxt "820\t900\t80\t　\t　\t "
msgid "I figured you'd show up."
msgstr "Immaginavo che ti saresti fatto vivo."

msgctxt "923\t1005\t82\t　\t　\t "
msgid "You guys are painfully predictable."
msgstr "Siete terribilmente prevedibili."

msgctxt "1028\t1088\t60\t　\t　\t "
msgid "Almost shocking."
msgstr "Quasi scioccante."

msgctxt "1103\t1155\t52\t　\t　\t60ｆ以下です"
msgid "Where's Majima?"
msgstr "Dov'è Majima?"

msgctxt "1156\t1216\t60\t分\t　\t "
msgid "Tell me where ya got Majima!"
msgstr "Dimmi dove hai nascosto Majima"

msgctxt "1958\t2018\t60\t　\t　\t "
msgid "Bro..."
msgstr "Fratello..."

msgctxt "2030\t2090\t60\t　\t　\t "
msgid "Majima!"
msgstr "Majima!"

msgctxt "2179\t2250\t71\t　\t　\t "
msgid "I messed up, bro."
msgstr "Ho fatto un casino, fratello."

msgctxt "2293\t2410\t117\t　\t　\t "
msgid "I'm sorry... I let it get to this."
msgstr "Perdonami... ho lasciato che si arrivasse a questo."

msgctxt "2473\t2580\t107\t　\t　\t "
msgid "Kurosawa! What the hell's this about!?"
msgstr "Kurosawa! Che diavolo significa tutto questo!?"

msgctxt "2581\t2641\t60\t　\t　\t "
msgid "Start talkin'!"
msgstr "Sputa il rospo"

msgctxt "2673\t2733\t60\t　\t　\t "
msgid "All in good time."
msgstr "Tutto a tempo debito."

msgctxt "2844\t2913\t69\t　\t　\t "
msgid "The best part's still to come."
msgstr "Il bello deve ancora venire."

msgctxt "3648\t3719\t71\t　\t　\t "
msgid "What're you playin' at?"
msgstr "A che gioco stai giocando?"

msgctxt "3745\t3805\t60\t　\t　\t "
msgid "There we go."
msgstr "Ecco fatto."

msgctxt "3812\t3872\t60\t　\t　\t "
msgid "Hey."
msgstr "Ehi."

msgctxt "4105\t4183\t78\t　\t　\t "
msgid "Okay. Start the show."
msgstr "Okay. Date inizio allo spettacolo."

msgctxt "4458\t4598\t140\t　\t　\t "
msgid ""
"Majima... Hey...\r\n"
"What're ya doin'?"
msgstr ""
"Majima... Ehi...\n"
"Che stai facendo?"

msgctxt "4632\t4698\t66\t　\t　\t "
msgid "Sorry, brother."
msgstr "Mi dispiace, fratello."

msgctxt "4752\t4880\t128\t　\t　\t "
msgid "Seems we gotta try to kill each other."
msgstr "Sembra che dobbiamo provare a ucciderci a vicenda."

msgctxt "5029\t5072\t43\t　\t　\t60ｆ以下です"
msgid "What the hell?"
msgstr "Ma che diavolo?"

msgctxt "5073\t5133\t60\t　\t　\t "
msgid "I couldn't do it..."
msgstr "Non ce l'ho fatta..."

msgctxt "5152\t5247\t95\t　\t　\t "
msgid "She means more to Kiryu-chan than life itself."
msgstr "Per Kiryu-chan, lei vale più della sua stessa vita."

msgctxt "5286\t5358\t72\t　\t　\t "
msgid "I couldn't just let it happen."
msgstr "Non potevo permettere che accadesse."

msgctxt "5392\t5480\t88\t　\t　\t "
msgid "You're not makin' a goddamn lick of sense!"
msgstr "Quello che dici non ha un cazzo di senso"

msgctxt "5495\t5555\t60\t　\t　\t "
msgid "This'll clear things up."
msgstr "Questo chiarirà le cose."
