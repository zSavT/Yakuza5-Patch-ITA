msgstr "Uda"

msgctxt "5908"
msgid "Talk_Call"
msgstr "Talk_Call"

msgctxt "5912"
msgid "Haruka"
msgstr "Haruka"

msgctxt "5916"
msgid "Talk_Ojigi"
msgstr "Talk_Ojigi"

msgctxt "5920"
msgid "Talk_Think"
