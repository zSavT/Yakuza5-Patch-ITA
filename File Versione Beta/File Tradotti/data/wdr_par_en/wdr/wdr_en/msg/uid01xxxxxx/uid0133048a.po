msgctxt "1188"
msgid "Player"
msgstr "Giocatore"

msgctxt "1192"
msgid "Play"
msgstr "Gioca"

msgctxt "1196"
msgid "Don't Play"
msgstr "Non giocare"

msgctxt "60_Player"
msgid "(The Mission Course costs 300 yen to play. Should I go for it?)"
msgstr "(Il percorso missione costa 300 yen per giocare. Ci sto?)"

msgctxt "84_Player"
msgid "(Oops. I'm too broke.)"
msgstr "(Ops. Sono troppo al verde.)"

msgctxt "96_Player"
msgid "(All right. Let's do this.)"
msgstr "(Ok. Ci sto!)"

msgctxt "120_Player"
msgid "(Nope. Not now.)"
msgstr "(Nah. Non ora.)"

msgctxt "132_Player"
msgid "(The level's too high. It's a bit more than I can handle now.)"
msgstr ""
"(Il livello è troppo alto. È un po' più di quello che posso gestire ora.)"
