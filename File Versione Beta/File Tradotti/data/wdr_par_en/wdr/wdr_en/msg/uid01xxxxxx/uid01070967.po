msgctxt "2776"
msgid "Shinada"
msgstr "Shinada"

msgctxt "380_Shinada"
msgid "Let's decide who's banker."
msgstr "Decidiamo chi è il banchiere."

msgctxt "392_Shinada"
msgid "Looks like I'm the banker."
msgstr "Mi sa che sono io il banchiere."

msgctxt "404_Shinada"
msgid "(How much should I bet?)"
msgstr "(Quanto dovrei puntare?)"

msgctxt "416_Shinada"
msgid "Okay, here goes..."
msgstr "Ok, ci siamo..."

msgctxt "428_Shinada"
msgid "My turn."
msgstr "Tocca a me."

msgctxt "440_Shinada"
msgid "I'll roll once more."
msgstr "Faccio un altro tiro."

msgctxt "452_Shinada"
msgid "Time to settle up."
msgstr "È ora di saldare i conti."

msgctxt "464_Shinada"
msgid "(You're the banker.)"
msgstr "(Sei il banchiere.)"

msgctxt "476_Shinada"
msgid "Now we change the banker."
msgstr "Ora cambiamo il banchiere."

msgctxt "488_Shinada"
msgid "(Play again?)"
msgstr "(Facciamo un altro giro?)"

msgctxt "500_Shinada"
msgid "(I'm broke.)"
msgstr "(Sono al verde.)"

msgctxt "512_Shinada"
msgid "(Try again next time.)"
msgstr "(Riprova la prossima volta.)"

msgctxt "524_Shinada"
msgid "Banker's Bet-Settling Messages"
msgstr "Messaggi di saldatura puntate del banchiere"

msgctxt "536_Shinada"
msgid "Was a banker roll of 1-3-5 a 3?"
msgstr "Il lancio del banchiere di 1-3-5 era un 3?"

msgctxt "548_Shinada"
msgid "Was a 1-3-5 by the first player a 3?"
msgstr "Il lancio del primo giocatore di 1-3-5 era un 3?"

msgctxt "560_Shinada"
msgid "Does 1-3-5 become the previous player's roll?"
msgstr "Il 1-3-5 diventa il lancio del giocatore precedente?"

msgctxt "572_Shinada"
msgid "Was a banker roll of 2-4-6 a 4?"
msgstr "Il lancio del banchiere di 2-4-6 era un 4?"

msgctxt "584_Shinada"
msgid "Does 2-4-6 become the next player's roll?"
msgstr "Il 2-4-6 diventa il lancio del giocatore successivo?"

msgctxt "596_Shinada"
msgid "Was 2-4-6 by the last player a 4?"
msgstr "Il lancio dell'ultimo giocatore di 2-4-6 era un 4?"

msgctxt "608_Shinada"
msgid "A 2-4-6 becomes the last player's roll, right?"
msgstr "Il 2-4-6 diventa il lancio dell'ultimo giocatore, vero?"

msgctxt "620_Shinada"
msgid "A 1-3-5 was rolled after a 2-4-6."
msgstr "Un 1-3-5 è uscito dopo un 2-4-6."

msgctxt "632_Shinada"
msgid "Oh, yeah! A 6-3-4!"
msgstr "Oh, sì! Un 6-3-4!"
