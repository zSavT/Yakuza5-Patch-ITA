msgctxt "140"
msgid "Welcome to the poker table."
msgstr "Benvenuto al tavolo da poker."

msgctxt "152"
msgid "Let's begin."
msgstr "Iniziamo."

msgctxt "164"
msgid "We look forward to seeing you again."
msgstr "Speriamo di rivederla."

msgctxt "176"
msgid "You seem to be short on chips. Please visit us again soon."
msgstr "Sembra che tu sia a corto di fiches. Torna a trovarci presto."

msgctxt "188"
msgid "Choose a card to discard."
msgstr "Scegli una carta da scartare."

msgctxt "200"
msgid "(The item's effect has worn off.)"
msgstr "L'effetto dell'oggetto Ã¨ svanito."

msgctxt "212"
msgid "You are on quite a roll, sir."
msgstr "Hai proprio la mano calda, signore."

msgctxt "224"
msgid "You are doing great, sir."
msgstr "Stai andando alla grande, signore."
