msgctxt "2796"
msgid "Thick-cut Tetsu"
msgstr "Thick-cut Tetsu"

msgctxt "380_Thick-cut Tetsu"
msgid "First, we pick the banker."
msgstr "Per prima cosa, scegliamo il banco."

msgctxt "392_Thick-cut Tetsu"
msgid "I'm the banker."
msgstr "Sono il banchiere."

msgctxt "404_Thick-cut Tetsu"
msgid "How much will you bet?"
msgstr "Quanto vuoi puntare?"

msgctxt "416_Thick-cut Tetsu"
msgid "Let's get started."
msgstr "Iniziamo."

msgctxt "428_Thick-cut Tetsu"
msgid "My turn."
msgstr "Tocca a me."

msgctxt "440_Thick-cut Tetsu"
msgid "(This line should not appear.)"
msgstr "(Questa riga non dovrebbe apparire.)"

msgctxt "452_Thick-cut Tetsu"
msgid "Let's settle our bets."
msgstr "Regoliamo i conti."

msgctxt "464_Thick-cut Tetsu"
msgid "You're the banker."
msgstr "Sei il banchiere."

msgctxt "476_Thick-cut Tetsu"
msgid "Time to change the banker."
msgstr "Si cambia il banco."

msgctxt "488_Thick-cut Tetsu"
msgid "Want to play again?"
msgstr "Vuoi giocare ancora?"

msgctxt "500_Thick-cut Tetsu"
msgid "I'm tapped out. Good time to head on home."
msgstr "Sono al verde. È ora di tornare a casa."

msgctxt "512_Thick-cut Tetsu"
msgid "That it for you? Better luck next time."
msgstr "È finita qui per te? Più fortuna la prossima volta."

msgctxt "524_Thick-cut Tetsu"
msgid "Banker's Bet-Settling Messages"
msgstr "Messaggi del banchiere per il regolamento dei conti"

msgctxt "536_Thick-cut Tetsu"
msgid "A banker's roll of 1-3-5 becomes a 3."
msgstr "Il lancio del banchiere di 1-3-5 diventa un 3."

msgctxt "548_Thick-cut Tetsu"
msgid "A 1-3-5 roll by the first player becomes a 3."
msgstr "Il lancio di 1-3-5 del primo giocatore diventa un 3."

msgctxt "560_Thick-cut Tetsu"
msgid "A 1-3-5 roll gets the preceding player's roll."
msgstr "Il lancio di 1-3-5 diventa il lancio del giocatore precedente."

msgctxt "572_Thick-cut Tetsu"
msgid "A banker's roll of 2-4-6 becomes a 4."
msgstr "Il lancio del banchiere di 2-4-6 diventa un 4."

msgctxt "584_Thick-cut Tetsu"
msgid "A 2-4-6 roll becomes the next player's roll."
msgstr "Il lancio di 2-4-6 diventa il lancio del giocatore successivo."

msgctxt "596_Thick-cut Tetsu"
msgid "A 2-4-6 roll by the last player becomes a 4."
msgstr "Il lancio di 2-4-6 dell'ultimo giocatore diventa un 4."

msgctxt "608_Thick-cut Tetsu"
msgid "A 2-4-6 roll gets the last player's roll!"
msgstr "Il lancio di 2-4-6 diventa il lancio dell'ultimo giocatore!"

msgctxt "620_Thick-cut Tetsu"
msgid "A 1-3-5 was rolled after a 2-4-6."
msgstr "È uscito un 1-3-5 dopo un 2-4-6."

msgctxt "632_Thick-cut Tetsu"
msgid "A 6-3-4! Sweet..."
msgstr "Un 6-3-4! Ottimo..."
