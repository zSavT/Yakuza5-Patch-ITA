msgctxt "1676_Hinata"
msgid "Oh, you like that, huh? <Color:255,200,200,255>♥<Color:Default>"
msgstr "Oh, ti piace, eh? <Color:255,200,200,255>♥<Color:Default>"

msgctxt "1688_Hinata"
msgid "Okay, sure thing."
msgstr "Okay, certo."

msgctxt "1700_Hinata"
msgid "You like that? Oh."
msgstr "Ti piace? Oh."

msgctxt "1712_Hinata"
msgid "Okay. Maybe some other time."
msgstr "Okay. Magari un'altra volta."

msgctxt "1748_Hinata"
msgid "Hey, want to play the chocolate stick game?"
msgstr "Ehi, ti va di fare il gioco del bastoncino di cioccolato?"

msgctxt "1760_Player"
msgid "Sure."
msgstr "Certo."

msgctxt "1796_Hinata"
msgid "Okay, let me feed it to you. <Color:255,200,200,255>♥<Color:Default>"
msgstr "Okay, lascia che ti imbocchi. <Color:255,200,200,255>♥<Color:Default>"

msgctxt "1808_Player"
msgid "What!?"
msgstr "Cosa!?"

msgctxt "1904_Hinata"
msgid "Over here, please."
msgstr "Da questa parte, per favore."

msgctxt "2000_Hinata"
msgid "Let me mix that for you."
msgstr "Lascia che te lo prepari io."

msgctxt "2012_Player"
msgid "Okay."
msgstr "Ok."

msgctxt "2024_Hinata"
msgid "Drink up!"
msgstr "Bevi tutto!"

msgctxt "2036_Hinata"
msgid "You sure drink a lot!"
msgstr "Certo che bevi un sacco!"

msgctxt "2048_Player"
msgid "I suppose."
msgstr "Immagino."

