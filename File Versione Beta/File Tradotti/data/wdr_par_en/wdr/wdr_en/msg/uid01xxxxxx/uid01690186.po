msgstr "Regista"

msgctxt "7216"
msgid "Talk_OK"
msgstr "Talk_OK"

msgctxt "7220"
msgid "I could use a refresher."
msgstr "Mi servirebbe un ripasso."

msgctxt "7224"
msgid "Yep, I'm all set."
msgstr "Sì, sono pronta."

msgctxt "7228"
msgid "Haruka"
msgstr "Haruka"

