msgstr "Sei fortunata, Eri."

msgctxt "27016"
msgid "2d_yk_sstory_A01_riona"
msgstr "2d_yk_sstory_A01_riona"

msgctxt "27020"
msgid "It's Christmas time, Shiro."
msgstr "È Natale, Shiro."

msgctxt "27024"
