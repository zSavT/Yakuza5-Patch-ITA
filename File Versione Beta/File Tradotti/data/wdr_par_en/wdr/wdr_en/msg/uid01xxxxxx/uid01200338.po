msgctxt "1424"
msgid "受け答え"
msgstr "受け答え"

msgctxt "1428"
msgid "Wada"
msgstr "Wada"

msgctxt "1432"
msgid "○I've learned the ropes."
msgstr "○Ho imparato il mestiere."

msgctxt "1436"
msgid "△Still getting the hang of things."
msgstr "△Sto ancora prendendo la mano."

msgctxt "1440"
msgid "□I have no idea what I'm doing."
msgstr "□Non so dove mettere le mani."

msgctxt "1444"
msgid "Kiryu"
msgstr "Kiryu"

msgctxt "56_Wada"
msgid "You all settled in at work now?"
msgstr "Ti sei ambientato a lavoro ormai?"

msgctxt "80_受け答え"
msgid "Yes. I've pretty much learned the ropes."
msgstr "Sì. Ho praticamente imparato il mestiere."

msgctxt "92_Wada"
msgid "Good to hear."
msgstr "Bello a sapersi."

msgctxt "104_受け答え"
msgid "I'm still trying to get the hang of things."
msgstr "Sto ancora cercando di prendere la mano."

msgctxt "116_Wada"
msgid "Don't worry, you'll get there."
msgstr "Non preoccuparti, ci arriverai."

msgctxt "128_受け答え"
msgid "I have no idea what I'm doing."
msgstr "Non so dove mettere le mani."

msgctxt "140_Wada"
msgid "Hm. Well, I hope you find your feet soon."
msgstr "Mh. Beh, spero che ti ambienti presto."

msgctxt "152_受け答え"
msgid "Hey, don't ignore the question!"
msgstr "Ehi, non ignorare la domanda!"
