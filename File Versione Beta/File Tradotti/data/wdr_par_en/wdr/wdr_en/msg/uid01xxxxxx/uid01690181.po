msgstr "Regista"

msgctxt "7200"
msgid "Talk_OK"
msgstr "Talk_OK"

msgctxt "7204"
msgid "I could use a refresher."
msgstr "Mi serve una rinfrescata."

msgctxt "7208"
msgid "Yep, I'm all set."
msgstr "Sì, sono a posto."

msgctxt "7212"
