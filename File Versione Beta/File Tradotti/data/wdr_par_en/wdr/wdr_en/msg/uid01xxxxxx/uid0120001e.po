msgid "走ってきた男"
msgstr "走ってきた男"

msgctxt "3600"
msgid "男性"
msgstr "男性"

msgctxt "3604"
msgid "ストーカー"
msgstr "ストーカー"

msgctxt "44_桐生"
msgid "ん……？"
msgstr "ん……？"

msgctxt "68_走ってきた男"
msgid "ハァ！　ハァ！　ハァ……！"
msgstr "ハァ！　ハァ！　ハァ……！"

msgctxt "80_走ってきた男"
msgid "フゥー……　フゥー……"
msgstr "フゥー……　フゥー……"

msgctxt "92_走ってきた男"
msgid ""
"あ、あんたらここらに超可愛い\r\n"
"アイドル顔負けの女性がこなかった!?"
msgstr ""
"あ、あんたらここらに超可愛い\n"
"アイドル顔負けの女性がこなかった!?"

msgctxt "104_桐生"
msgid "（こいつがさっきの彼女が話していたストーカーか）"
msgstr "（こいつがさっきの彼女が話していたストーカーか）"

msgctxt "116_桐生"
msgid "知らないな。　アンタ見たか？"
msgstr "知らないな。　アンタ見たか？"

msgctxt "128_男性"
msgid "いえ、知りませんね。"
msgstr "いえ、知りませんね。"

msgctxt "140_ストーカー"
msgid ""
"フゥー……　フゥー……\r\n"
"た、たしかにこっちに来たはずなのに……"
msgstr ""
"フゥー……　フゥー……\n"
"た、たしかにこっちに来たはずなのに……"

msgctxt "152_ストーカー"
msgid ""
"涼子チャン、恥ずかしがり屋だから\r\n"
"ボクがいないとダメなんだ。"
msgstr ""
"涼子チャン、恥ずかしがり屋だから\n"
"ボクがいないとダメなんだ。"

msgctxt "164_ストーカー"
msgid ""
"でもあんまり困らせるようなら\r\n"
"フゥー……　フゥー……"
msgstr ""
"でもあんまり困らせるようなら\n"
"フゥー……　フゥー……"

msgctxt "176_ストーカー"
msgid ""
"ボクにも……　ボクにもネェー！\r\n"
"考えがあるんだよォー!?"
msgstr ""
"ボクにも……　ボクにもネェー！\n"
"考えがあるんだよォー!?"

msgctxt "188_桐生"
msgid ""
"（……まずいな。　これは彼女に気害を及ぼしそうだ。\r\n"
"　嘘の情報を与えておいたほうがいいだろう）"
msgstr ""
"（……まずいな。　これは彼女に気害を及ぼしそうだ。\n"
"　嘘の情報を与えておいたほうがいいだろう）"

msgctxt "200_桐生"
msgid ""
"そういえばさっき若い女が\r\n"
"あっちに走っていったのを見たな。"
msgstr ""
"そういえばさっき若い女が\n"
"あっちに走っていったのを見たな。"

msgctxt "212_ストーカー"
msgid "えっ！"
msgstr "えっ！"

msgctxt "224_ストーカー"
msgid ""
"フゥー……　フゥー……\r\n"
"愛しの涼子チャン……"
msgstr ""
"フゥー……　フゥー……\n"
"愛しの涼子チャン……"

msgctxt "236_ストーカー"
msgid ""
"今日こそ、君のお家を教えてもらうからね\r\n"
"デュフフ……"
msgstr ""
"今日こそ、君のお家を教えてもらうからね\n"
"デュフフ……"

msgctxt "260_桐生"
msgid ""
"これで少しは安心だろう。\r\n"
"アンタも災難だったな。"
msgstr ""
"これで少しは安心だろう。\n"
"アンタも災難だったな。"

msgctxt "272_男性"
msgid ""
"いえ。　一人の女性を助けたと思えば\r\n"
"気にもなりませんよ。"
msgstr ""
"いえ。　一人の女性を助けたと思えば\n"
"気にもなりませんよ。"

msgctxt "284_男性"
msgid ""
"さて、次のタクシーを私も捕まえないと。\r\n"
"では。"
msgstr ""
"さて、次のタクシーを私も捕まえないと。\n"
"では。"

msgctxt "308_桐生"
msgid ""
"（大山さんも無事に彼女を\r\n"
"　送り届けられているといいが……）"
msgstr ""
"（大山さんも無事に彼女を\n"
"　送り届けられているといいが……）"
