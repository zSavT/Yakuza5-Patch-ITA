msgctxt "2572"
msgid "Suited Man"
msgstr "Uomo in completo"

msgctxt "2576"
msgid "dummy"
msgstr "dummy"

msgctxt "2580"
msgid "○I'll give it a shot."
msgstr "○Ci provo."

msgctxt "2584"
msgid "△I can't do that."
msgstr "△Non posso farlo."

msgctxt "2588"
msgid "□I'll go as fast as I safely can."
msgstr "□Andrò il più veloce che posso, ma in sicurezza."

msgctxt "2592"
msgid "○I'll try tailgating them."
msgstr "○Ci provo a stargli attaccato."

msgctxt "2596"
msgid "△Let's run them off the road."
msgstr "△Buttiamoli fuori strada."

msgctxt "2600"
msgid "□I'll see if I can pass them."
msgstr "□Ci provo a superarli."

msgctxt "108_Suited Man"
msgid "Uh-oh. I'm out of time."
msgstr "Ops. Non ho più tempo."

msgctxt "120_Suited Man"
msgid "No, no. This won't do at all. Can you just let me out here?"
msgstr "No, no. Così non va bene per niente. Mi puoi far scendere qui?"

msgctxt "132_Suited Man"
msgid "As fast as you can."
msgstr "Il più veloce che puoi."

msgctxt "144_Suited Man"
msgid "Make that super fast."
msgstr "Anzi, a tutta birra."

msgctxt "156_Suited Man"
msgid "Triple the speed limit would be even better."
msgstr "Il triplo del limite di velocità sarebbe ancora meglio."

msgctxt "180_Suited Man"
msgid "What!? I was only joking!"
msgstr "Cosa!? Stavo solo scherzando!"

msgctxt "192_Suited Man"
msgid "I know. It was just a joke."
msgstr "Lo so. Era solo uno scherzo."

msgctxt "204_Suited Man"
msgid "Yeah, that'd be best."
msgstr "Sì, la cosa migliore."

msgctxt "216_Suited Man"
msgid "Huh? Are you even listening?"
msgstr "Eh? Mi stai ascoltando?"

msgctxt "228_Suited Man"
msgid "Ugh, that car is going so slow!"
msgstr "Ugh, 'sta macchina va lentissima!"

msgctxt "240_Suited Man"
msgid "What the hell's their problem?"
msgstr "Ma che diavolo hanno?"

msgctxt "264_Suited Man"
msgid "Just don't trigger any road rage."
msgstr "Basta che non scateni la rabbia al volante."

msgctxt "276_Suited Man"
msgid "Huh!? That's taking it too far."
msgstr "Cosa!? Stai esagerando."

msgctxt "288_Suited Man"
msgid "Please do!"
msgstr "Fallo pure!"

msgctxt "300_Suited Man"
msgid "Hmm? Didn't you hear me?"
msgstr "Eh? Ma non mi hai sentito?"
