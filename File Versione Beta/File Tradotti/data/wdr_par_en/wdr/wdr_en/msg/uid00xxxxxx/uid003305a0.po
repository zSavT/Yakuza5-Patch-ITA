msgctxt "44_Employee"
msgid "Oh, hello! Right this way, please."
msgstr "Oh, salve! Di qua, prego."

msgctxt "92_Kiryu"
msgid "(I don't need to eat now.)"
msgstr "(Non ho bisogno di mangiare ora.)"

msgctxt "104_Akiyama"
msgid "(I'm not hungry.)"
msgstr "(Non ho fame.)"

msgctxt "116_Saejima"
msgid "(I'm full for now.)"
msgstr "(Sono sazio per ora.)"

msgctxt "128_Shinada"
msgid "(It's tempting, but I'm stuffed.)"
msgstr "(È allettante, ma sono sazio.)"

msgctxt "140_Haruka"
msgid "(I'm full.)"
msgstr "(Sono sazio.)"

msgctxt "164_Player"
msgid "(I'm out of cash.)"
msgstr "(Sono senza soldi.)"

msgctxt "200_Kiryu"
msgid "Sorry, I have to run."
msgstr "Scusa, devo scappare."

msgctxt "212_Akiyama"
msgid "Sorry, I forgot an urgent piece of business."
msgstr "Scusa, ho dimenticato un affare urgente."

msgctxt "224_Saejima"
msgid "Sorry, gotta go."
msgstr "Scusa, devo scappare."

msgctxt "236_Shinada"
msgid "Oh, sorry! I have to run."
msgstr "Oh, scusa! Devo scappare."

msgctxt "248_Haruka"
msgid "I'm so sorry. Something came up."
msgstr "Mi dispiace tanto. È successo qualcosa."

msgctxt "260_Employee"
msgid "All right. Please do come again."
msgstr "Va bene. Torni a trovarci."

msgctxt "368_Employee"
msgid "May I take your order?"
msgstr "Posso prendere la sua ordinazione?"

msgctxt "392_Employee"
msgid "What would you like to eat?"
msgstr "Cosa ti piacerebbe mangiare?"

msgctxt "440_Employee"
msgid "Here is your check. Thank you very much."
msgstr "Ecco il suo conto. Grazie mille."

msgctxt "476_Employee"
msgid "What would you like to eat?"
msgstr "Cosa ti piacerebbe mangiare?"

msgctxt "524_Saejima"
msgid "(Well, maybe I oughta eat somethin' and savor the moment.)"
msgstr "(Beh, forse dovrei mangiare qualcosa e godermi il momento.)"

msgctxt "536_Employee"
msgid "Sorry, just remembered I got something to do first."
msgstr "Scusa, mi è appena tornato in mente che devo fare una cosa prima."

msgctxt "548_Employee"
msgid "I see. Well, please come again."
msgstr "Capisco. Beh, torni pure."

msgctxt "596_Employee"
msgid "Thank you very much. We hope to see you again soon."
msgstr "Grazie mille. Speriamo di rivederla presto."
