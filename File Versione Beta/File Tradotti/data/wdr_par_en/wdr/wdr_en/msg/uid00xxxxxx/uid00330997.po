msgid "You got <Color:8>20,000 yen<Color:Default>."
msgstr "Hai intascato <Color:8>20.000 yen<Color:Default>."

msgctxt "376"
msgid "You got a <Color:8>Borrowed Single-lens Reflex Camera<Color:Default>."
msgstr ""
"Hai preso una <Color:8>Macchina fotografica reflex in "
"prestito<Color:Default>."

msgctxt "7876"
msgid "Tourism Board Employee"
msgstr "Impiegato dell'Ente del Turismo"

msgctxt "7880"
msgid "Player"
msgstr "Giocatore"

