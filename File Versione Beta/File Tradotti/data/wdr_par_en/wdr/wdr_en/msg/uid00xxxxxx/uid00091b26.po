msgctxt "2036"
msgid "Saejima"
msgstr "Saejima"

msgctxt "2040"
msgid "Judgmental Son"
msgstr "Figlio giudicante"

msgctxt "2044"
msgid "Talk_Ase"
msgstr "Talk_Ase"

msgctxt "2048"
msgid "Talk_Ojigi"
msgstr "Talk_Ojigi"

msgctxt "76_Saejima"
msgid "(There he is.)"
msgstr "(Eccolo.)"

msgctxt "124_Saejima"
msgid "Hey, ya alright?"
msgstr "Ehi, tutto a posto?"

msgctxt "136_Judgmental Son"
msgid "The outsider!"
msgstr "L'estraneo!"

msgctxt "148_Saejima"
msgid "...Yer attitude seems to be alright, at least."
msgstr "...Il tuo atteggiamento, almeno, mi sembra decente."

msgctxt "160_Judgmental Son"
msgid "Have you seen my mother? We got separated."
msgstr "Hai visto mia madre? Ci siamo separati."

msgctxt "172_Saejima"
msgid "Yeah. She's fine."
msgstr "Sì. Sta bene."

msgctxt "184_Judgmental Son"
msgid "Ah, what a relief."
msgstr "Ah, che sollievo."

msgctxt "196_Saejima"
msgid "Yeah. Let's meet up with 'er and go back down."
msgstr "Sì. Incontriamola e torniamo giù."

msgctxt "208_Judgmental Son"
msgid "Oh, um..."
msgstr "Oh, ehm..."

msgctxt "220_Saejima"
msgid "Hurry, before she wanders off lookin' for us."
msgstr "Sbrigati, prima che si metta a cercarci in giro."

msgctxt "232_Judgmental Son"
msgid "Okay."
msgstr "Ok."
